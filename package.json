    "axios": "^0.21.1",
    "cors": "^2.8.5",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "if-env": "^1.0.4",
    "mysql": "^2.17.1",
    "nodejs-latest": "^1.1.0"
